{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../.vercel/output/functions",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": false
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
